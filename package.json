{
  "name": "@meadow/react-slider",
  "version": "3.0.0-alpha.1",
  "description": "Clean and simple Slider component. For when <input type=\"range\" /> just won't do.",
  "main": "dist/components/slider.js",
  "scripts": {
    "prepublish": "npm run build",
    "postpublish": "npm run clean",
    "build": "mkdirp dist/styles && stylus src/styles --out dist/styles && mkdirp dist/components && babel src/components --out-dir dist/components",
    "clean": "trash lib",
    "test": "zuul -- test/**/*.{js,jsx}",
    "test:browser": "zuul --local 55555 -- test/**/*.{js,jsx}",
    "lint": "eslint src/components --ext .js --ext .jsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meadow/react-slider.git"
  },
  "keywords": [
    "react",
    "react-component",
    "component",
    "slider",
    "range",
    "input",
    "meadow"
  ],
  "author": "Timothy Kempf <tim@getmeadow.com> (https://getmeadow.com)",
  "license": null,
  "bugs": {
    "url": "https://github.com/meadow/react-slider/issues"
  },
  "homepage": "https://github.com/meadow/react-slider#readme",
  "devDependencies": {
    "@babel/cli": "^7.1.0",
    "@babel/core": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@fauntleroy/trigger-event": "^1.0.1",
    "@meadow/eslint-config": "^5.0.3",
    "babelify": "10.0.0",
    "browserify": "^16.2.3",
    "dom-classes": "1.0.0",
    "eslint": "5.6.0",
    "mkdirp": "^0.5.1",
    "react": "^16.5.2",
    "react-addons-test-utils": "^0.14.8",
    "react-dom": "^16.5.2",
    "simple-mock": "^0.8.0",
    "stylus": "^0.54.5",
    "tape": "4.9.1",
    "trash": "^4.3.0",
    "watchify": "^3.11.0",
    "zuul": "3.12.0"
  },
  "peerDependencies": {
    "react": "0.14.x || 15.x || 16.x"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "create-react-class": "^15.6.3",
    "prop-types": "^15.6.2"
  }
}
